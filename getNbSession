# requests the notebook session. This must run on the server, as non-interactive ssh sessions 
# don't have access to all of the inteactive commands

# activate the virtual environment before requesting an interactive session
source venv/bin/activate
bsub -q interactive -Is "
# this is running on the interactive session
cd projects/$1
screen -d -m jupyter notebook --port=$2 --browser='none'
hostname
"


