# requests the notebook session. This must run on the server, as non-interactive ssh sessions 
# don't have access to all of the inteactive commands


# this runs on the login machine, and gets access to an interactive session

myHost=$( bsub -q interactive -Is "bash -l startSession $1")
hostAdr=$(echo $myHost | ack -o '(?<=\. ).*?$' )
echo $hostAdr
echo $1:127.0.0.1:$1
ssh -N -vv -L $1:127.0.0.1:$1 lr93@$hostAdr


